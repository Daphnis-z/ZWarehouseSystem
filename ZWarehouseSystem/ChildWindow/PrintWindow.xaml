<Window x:Class="ZWarehouseSystem.ChildWindow.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PrintWindow" Height="400" Width="485" ResizeMode="NoResize" WindowStyle="None" AllowsTransparency="True" Background="{x:Null}" MouseLeftButtonDown="Window_MouseLeftButtonDown" Initialized="Window_Initialized" WindowStartupLocation="CenterScreen">
    <Border BorderBrush="#FF37B208" BorderThickness="1" CornerRadius="2">
        <Border.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF37B208" Offset="0"/>
                <GradientStop Color="White" Offset="1"/>
                <GradientStop Color="#FF63C33E" Offset="0.108"/>
                <GradientStop Color="White" Offset="0.12"/>
            </LinearGradientBrush>
        </Border.Background>
        <Grid>
            <Label x:Name="LabTitle" Content="订货单" HorizontalAlignment="Center" Margin="188,3,166,0" VerticalAlignment="Top" Foreground="White" FontSize="17" Width="129"/>
            <Label x:Name="LabExit" Content="X" HorizontalAlignment="Center" Margin="460,3,5,0" VerticalAlignment="Top" Foreground="White" FontSize="14" MouseLeftButtonDown="LabExit_MouseLeftButtonDown"/>
            <TextBlock x:Name="TextBlockPrint" TextWrapping="Wrap" Text="TextBlock" Margin="50,49,5,5" FontSize="16"/>
        </Grid>
    </Border>
</Window>
